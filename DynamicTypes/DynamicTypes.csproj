<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>DynamicTypes</Title>
    <Authors>Robin Peters</Authors>
    <Company />
    <Description>Provides Classes to Create .Net Types at runtime</Description>
    <Copyright>Robin Peters</Copyright>
    <PackageProjectUrl>https://github.com/Discrepantia/DynamicTypes</PackageProjectUrl>
    <ApplicationIcon>DynamicTypesimg.ico</ApplicationIcon>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>reflection;runtimetypes;dynamictypes</PackageTags>
    <FileVersion></FileVersion>
    <AssemblyVersion></AssemblyVersion>
    <Version>1.0.3</Version>
    <PackageIcon>DynamicTypesimg.png</PackageIcon>
    <RepositoryUrl>https://github.com/Discrepantia/DynamicTypes</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
    <EmbeddedResource Include="..\DynamicTypesimg.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </EmbeddedResource>
    <EmbeddedResource Include="..\license.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </EmbeddedResource>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
